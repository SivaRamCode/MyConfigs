client
dev tun
persist-tun
proto tcp
#139.162.37.157
#www.google.com.my.ns3.usain.tk
#images.digi.com.my.ns1.usain.tk
remote www.google.com.my.ns3.usain.tk:110@mydigiapp.digi.com.my
http-proxy www.google.com.my.ns3.usain.tk 8000
persist-remote-ip
resolv-retry infinite
connect-retry 0 1
remote-cert-tls server
nobind
reneg-sec 0
keysize 0
rcvbuf 0
sndbuf 0
verb 2
comp-lzo
auth none
auth-nocache
cipher none
setenv CLIENT_CERT 0
http-proxy-option CUSTOM-HEADER "Host: web.digi.com.my"
http-proxy-option CUSTOM-HEADER "Connection: keep-alive"
http-proxy-option CUSTOM-HEADER "Proxy-Connection: keep-alive"
http-proxy-option CUSTOM-HEADER "Upgrade-Insecure-Requests: 1"
http-proxy-option CUSTOM-HEADER "DNT: 1"
http-proxy-option CUSTOM-HEADER "X-Forwarded-Host: global-turbo2.opera-mini.net:443"
http-proxy-option AGENT "Mozilla/5.0 (Linux; U; Android 4.4; en-us; Nexus 4 Build/JOP24G) AppleWebKit/534.30 (KHTML, like Gecko) Version/4.0 Mobile Safari/534.30"
http-proxy-option VERSION "1.1"
auth-user-pass
<ca>
